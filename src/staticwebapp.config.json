{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{jpg,gif,png}",
            "assets/*",
            "/*.{css,scss,js}"
        ]
    },
    "routes": [
        {
            "route": "/*",
            "headers": {
                "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
            }
        }
    ]
}